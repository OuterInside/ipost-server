{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "ipost/schema/getMailList.json",
  "title": "Tweet",
  "type": "object",
  "required": ["text"],
  "properties": {
    "list": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "time": {
            "type": "number",
            "description": "UNIX timestamp"
          },
          "imageUrl": {
            "type": "string",
            "description": "absolute URI"
          }
        }
      }
    }
  }
}
